{
    "manifest_version": 3,
    "name": "PPS Scheduler",
    "version": "1.0",
    "description": "Dynamically replace PPS Schedule based on Cloud Type",
    "permissions": [
        "activeTab",
        "storage",
        "tabs"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_start"
        }
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "icons": {
      "16": "schedule.png",
      "48": "schedule.png",
      "128": "schedule.png"
    }
}